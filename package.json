{
  "name": "@lampix/core",
  "version": "0.7.0",
  "description": "Lampix application development library",
  "repository": {
    "type": "git",
    "url": "https://github.com/lampix-org/lampixjs-core"
  },
  "main": "lib/lampix.js",
  "module": "lib/lampix.js",
  "types": "lib/src/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "assets"
  ],
  "scripts": {
    "docs": "typedoc --out docs src/index.ts --name Lampix.js --theme minimal --readme README.md",
    "build": "shx rm -rf ./lib && webpack",
    "prepublishOnly": "npm run build",
    "precommit": "lint-staged"
  },
  "keywords": [
    "lampix"
  ],
  "author": "Lampix",
  "license": "MIT",
  "devDependencies": {
    "husky": "0.14.3",
    "lint-staged": "6.0.0",
    "shx": "0.2.2",
    "ts-loader": "4.4.2",
    "tslint": "5.8.0",
    "tslint-config-airbnb": "5.4.2",
    "typedoc": "0.9.0",
    "typescript": "2.6.2",
    "webpack": "4.16.3",
    "webpack-cli": "3.1.0"
  },
  "lint-staged": {
    "*.ts": [
      "tslint"
    ]
  },
  "dependencies": {}
}
